{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
  },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
          "devDependencies": true
      }
  ],
    "jsx-a11y/click-events-have-key-events": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/state-in-constructor": 0
  }
}
